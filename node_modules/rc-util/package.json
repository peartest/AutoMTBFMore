    }
  },
  "devDependencies": {
    "coveralls": "^2.11.15",
    "create-react-class": "^15.6.3",
    "expect.js": "~0.3.1",
    "jest": "20.x",
    "pre-commit": "^1.0.7",
    "rc-tools": "6.x",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "add-dom-event-listener": "^1.1.0",
    "babel-runtime": "6.x",
    "prop-types": "^15.5.10",
    "shallowequal": "^0.2.2"
  }
