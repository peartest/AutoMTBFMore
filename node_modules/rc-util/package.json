{
  "name": "rc-util",
  "version": "4.6.0",
  "description": "Common Utils For React Component",
  "files": [
    "lib",
    "es"
  ],
  "homepage": "http://github.com/react-component/util",
  "repository": {
    "type": "git",
    "url": "git@github.com:react-component/util.git"
  },
  "licenses": "MIT",
  "config": {
    "port": 8100
  },
  "jest": {
    "collectCoverageFrom": [
      "src/*"
    ],
